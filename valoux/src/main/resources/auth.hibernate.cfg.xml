<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<!-- All Models class must be define here -->
<hibernate-configuration>
	<session-factory>
		<mapping class="com.valoux.bean.LoginBean" />
		<mapping class="com.valoux.bean.UserTypeBean" />
		<mapping class="com.valoux.bean.UserRoleBean" />
		<mapping class="com.valoux.bean.MasterRoleBean" />
		<mapping class="com.valoux.bean.LoginLogBean" />

	</session-factory>
</hibernate-configuration>